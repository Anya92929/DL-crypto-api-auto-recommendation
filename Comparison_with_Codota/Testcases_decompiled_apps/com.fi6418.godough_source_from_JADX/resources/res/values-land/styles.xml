<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Check.Image">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">95dp</item>
    </style>
    <style name="CheckImageText">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#dfdfdf</item>
        <item name="android:background">#66000000</item>
        <item name="android:padding">8dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#cc000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>
    <style name="HomeLogo">
        <item name="android:layout_height">55dp</item>
    </style>
    <style name="SplashLogo">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginBottom">65dp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="TextAppearance.AppCompat.Widget.ActionBar.Subtitle.Inverse.GoDough">
        <item name="android:textSize">10sp</item>
        <item name="android:paddingBottom">0dp</item>
    </style>
    <style name="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse.GoDough">
        <item name="android:textSize">14sp</item>
    </style>
    <style name="Widget.Design.TabLayout">
        <item name="tabMode">1</item>
        <item name="tabGravity">1</item>
    </style>
    <style name="CheckInfo">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#ffffff</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>
</resources>
